{
  "version": 2,
  "builds": [
    {       
      "src": "dist/index.js",       
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"]}
    }
  ],
  "routes":[
    {
      "src": "/",
      "status": 308,
      "headers": { "Location": "/topics" }
    },
    {         
      "src": "/(.*)",
      "dest": "dist/index.js"
    }
  ]
}
